{
  "name": "adventure-of-sound",
  "version": "1.0.0",
  "description": "An interactive sound game",
  "main": "src/index.ts",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "build": "rm -rf dist && parcel build ./src/index.html --public-url='.' --no-source-maps",
    "clean": "rm -rf node_modules"
  },
  "keywords": ["p5", "ml5", "pitch-detection"],
  "author": "Billy Zou",
  "license": "GPL-3.0",
  "dependencies": {
    "ml5": "^0.4.3",
    "p5": "^0.9.0"
  },
  "devDependencies": {
    "@types/p5": "^0.9.0",
    "parcel": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.7.2"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
